{
  "name": "codesandbox-nuxt",
  "version": "1.0.0",
  "description": "Nuxt starter for CodeSandBox",
  "author": "Nuxt.js Team",
  "private": true,
  "scripts": {
    "dev": "MINIMAL=1 CSB=1 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest --verbose",
    "clear": "jest --clearCache"
  },
  "dependencies": {
    "@nuxt/http": "^0.1.0",
    "nuxt": "^2.6.1",
    "reqlim": "^0.0.0"
  },
  "devDependencies": {
    "fetch-mock": "^7.3.3",
    "flush-promises": "^1.0.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "vue-jest": "^3.0.3",
    "jest": "^24.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "vee-validate": "^2.2.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
